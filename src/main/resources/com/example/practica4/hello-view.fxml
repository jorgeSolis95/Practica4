<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.practica4.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <HBox alignment="TOP_RIGHT" spacing="10.0">
    </HBox>
    <Label fx:id="welcomeText"/>
    <HBox alignment="CENTER" spacing="20.0">
        <Button fx:id="btn_taco" text="Tacos"/>
        <Button fx:id="btn_bebida" text="Bebidas"/>
    </HBox>
    <Button fx:id="btn_orden" text="Ordenes"/>
    <Button fx:id="btn_pagar" text="Pagar Orden" onAction="#onHelloButtonClick"/>
</VBox>
